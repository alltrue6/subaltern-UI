/* 浮动 */
.float-left {
    float: left;
}

.float-right {
    float: right;
}

/* 清除浮动 */
.clear-float {
    clear: both;
}

/* 溢出隐藏 */
.overflow-hidden {
    width: 200rpx;
    /*限制宽度才可生效*/
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    /* display:-webkit-box;作为弹性伸缩盒子模型显示。 */
    /* -webkit-box-orient:vertical; 设置伸缩盒子的子元素排列方式--从上到下垂直排列 */
    /* -webkit-line-clamp:2; 显示的行 */
}
.child{
    width: calc(100%/4); /* 子盒子是父盒子的四分之一 */
}
/* 定位 */
.relative{
    position: relative;
}
.fixed{
    position: fixed;
}
.absolute{
    position: absolute;
}
/* 圆 */
.round{
    border-radius: 100%;
}
/* 圆角 */
.border-radius{
    border-radius: 20rpx;
}
/* 元素转换 */
.inline{
    display: inline;
}
.block{
    display: block;
}
.inline-block{
    display: inline-block;
}
